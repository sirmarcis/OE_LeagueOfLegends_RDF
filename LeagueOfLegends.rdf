<rdf:RDF xml:base="http://tw.rpi.edu/web/Courses/Ontologies/2017/LeagueOfLegends/LeagueOfLegends/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
     xmlns:fibo-fnd-utl-av="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"
     xmlns:fibo-fnd-utl-bt="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"
     xmlns:fibo-fnd-aap-agt="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"
     xmlns:fibo-fnd-rel-rel="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/"
     xmlns:fibo-fnd-utl-alx="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/Analytics/"
     xmlns:fibo-fnd-plc-loc="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Locations/"
     xmlns:fibo-fnd-plc-vrt="http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/"
     xmlns:fibo-fnd-pty-rl="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/"
     xmlns:fibo-fnd-pty-pty="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/"
     xmlns:fibo-fnd-aap-ppl="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/People/"
     xmlns:fibo-fnd-qt-qtu="http://www.omg.org/spec/EDMC-FIBO/FND/Quantities/QuantitiesAndUnits/"
     xmlns:fibo-fnd-acc-cur="http://www.omg.org/spec/EDMC-FIBO/FND/Accounting/CurrencyAmount/"
xmlns:lol="http://tw.rpi.edu/web/Courses/Ontologies/2017/LeagueOfLegends/LeagueOfLegends/">

    <owl:Ontology rdf:about="http://tw.rpi.edu/web/Courses/Ontologies/2017/LeagueOfLegends/LeagueOfLegends/">
        <rdfs:label>League of Legends Ontology</rdfs:label>

        <dct:publisher rdf:datatype="&xsd;string">LeagueOfLegends</dct:publisher>
        <dct:abstract rdf:datatype="&xsd;string">The objective of this ontology is to furnish a plugin to help a new player (secondary actor) learn the basics of playing a champion for the first time.  The basics include providing a list of recommended items to build based on the users, allied, and enemy champions, and a couple basic ability combinations the champion can do or terminology uniquely related to that champion. These suggested builds would continue throughout the game, depending on the amount of gold the user has, as well as the items the user has in their inventory at the time.</dct:abstract>

        <dct:creator rdf:datatype="&xsd;string">Hannah De los Santos</dct:creator>
        <dct:creator rdf:datatype="&xsd;string">Anders Maraviglia</dct:creator>

        <owl:versionIRI rdf:resource="http://tw.rpi.edu/web/Courses/Ontologies/2017/LeagueOfLegends/OE_10_LeagueOfLegends/"/>
        <dct:modified rdf:datatype="&xsd;dateTimeStamp">April 7, 2017</dct:modified>
        <skos:changeNote rdf:datatype="&xsd;string">added annotations and definitions to all classes and properties, as well as included provenance information</skos:changeNote>
        <sm:priorVersion rdf:datatype="&xsd;anyURI">http://tw.rpi.edu/web/Courses/Ontologies/2017/LeagueOfLegends/OE_8_LeagueOfLegends/</sm:priorVersion>
        <sm:priorVersion rdf:datatype="&xsd;anyURI">http://tw.rpi.edu/web/Courses/Ontologies/2017/LeagueOfLegends/OE_9_LeagueOfLegends/</sm:priorVersion>

        <owl:imports rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/AnnotationVocabulary/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/BusinessFacingTypes/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/Agents/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Relations/Relations/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Utilities/Analytics/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/Locations/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Places/VirtualPlaces/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Roles/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Parties/Parties/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/AgentsAndPeople/People/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Quantities/QuantitiesAndUnits/"/>
        <owl:imports rdf:resource="http://www.omg.org/spec/EDMC-FIBO/FND/Accounting/CurrencyAmount/"/>

    </owl:Ontology>
   

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdfs:Datatype rdf:about="&lol;GoldCost"/>

    <rdfs:Datatype rdf:about="&lol;ItemID"/>


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&lol;buildsInto">
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&lol;Item"/>
        <rdfs:comment>the item(s) that the current item can be upgraded to.  The value of hasTier must be greater than the current value for this relation to be valid. </rdfs:comment>
        <rdfs:label>builds into</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">sequence that items can be built in</skos:definition>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&lol;hasChampion">
        <rdfs:domain rdf:resource="&lol;Team"/>
        <rdfs:range rdf:resource="&lol;Champion"/>
        <dct:source rdf:datatype="&xsd;string">T. A. Tomato. (2016, December 6). LoL Essentials | League of Legends New Player/Beginner Guide (63rd ed.) [Online]. Available:http://www.mobafire.com/league-of-legends/build/lol-essentials-league-of-legends-new-player-beginner-guide-371292</dct:source>
        <rdfs:label>has champion</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">a player-controlled character, who begins each game at level 1 and has a maximum level of 18</skos:definition>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&lol;hasItem">
        <rdfs:domain rdf:resource="&lol;Champion"/>
        <rdfs:range rdf:resource="&lol;Item"/>
        <dct:source rdf:datatype="&xsd;integer">League of Legends Wiki. (2011, August). Items. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Item</dct:source>
        <rdfs:label>has item</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">artifact or object that is from Runeterran history and is featured in-game as an enhancement for a character</skos:definition>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&lol;hasStatistic">
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&lol;ItemSpecificStatistic"/>
        <dct:source rdf:datatype="&xsd;string">T. A. Tomato. (2016, December 6). LoL Essentials | League of Legends New Player/Beginner Guide (63rd ed.) [Online]. Available:http://www.mobafire.com/league-of-legends/build/lol-essentials-league-of-legends-new-player-beginner-guide-371292</dct:source>
        <rdfs:label>has statistic</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">one or more statistics that an item gives a champion </skos:definition>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&lol;hasUniqueItem">
        <rdfs:subPropertyOf rdf:resource="&lol;hasItem"/>
        <rdfs:domain rdf:resource="&lol;Champion"/>
        <rdfs:range rdf:resource="&lol;Item"/>
        <rdfs:label>has unique item
</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">item that only one champion can buy</skos:definition>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="&lol;inflictsDamageType">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&lol;Team"/>
                    <rdf:Description rdf:about="&fibo-fnd-pty-pty;PartyInRole"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&lol;DamageType"/>
        <rdfs:label>inflicts damage type</rdfs:label>
    </owl:ObjectProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:DatatypeProperty rdf:about="&lol;ArmorPenetration">
        <rdfs:domain rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:alternative rdf:datatype="&xsd;string">armor pen</dct:alternative>
        <dct:source rdf:datatype="&xsd;string">League of Legends Wiki. (2011, August). Armor Penetration. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Armor_penetration</dct:source>
        <rdfs:label>armor penetration</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">champion statistic that reduces the effectiveness of a target&apos;s armor</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;MagicPenetration">
        <rdfs:domain rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:alternative rdf:datatype="&xsd;string">magic pen</dct:alternative>
        <dct:source rdf:datatype="&xsd;string">League of Legends Wiki. (2011, August). Magic Penetration. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Magic_penetration</dct:source>
        <rdfs:label>magic penetration</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">champion statistic which allows the magic damage done from their abilities, enhanced attacks, etc. to ignore some or all of a target&apos;s magic resistance</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;SpellVamp">
        <rdfs:domain rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:label>spell vamp</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;Tenacity">
        <rdfs:domain rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:label>tenacity</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasAdvancedTier">
        <rdfs:subPropertyOf rdf:resource="&lol;hasTier"/>
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:source rdf:datatype="&xsd;integer">League of Legends Wiki. (2011, August). Items. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Item</dct:source>
        <rdfs:label>has advanced tier</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">item that has a specified recipe that consists of basic items and a combine cost</skos:definition>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="&lol;hasBaseStatisticValue">
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
        <rdfs:domain rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
        <dct:alternative rdf:datatype="&xsd;string">base stats</dct:alternative>
        <rdfs:label>has base statistic value</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">values that are default for champions</skos:definition>
        <skos:example rdf:datatype="&xsd;string">Ashe starts out with 325 movement speed</skos:example>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasBasicTier">
        <rdfs:subPropertyOf rdf:resource="&lol;hasTier"/>
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:source rdf:datatype="&xsd;integer">League of Legends Wiki. (2011, August). Items. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Item</dct:source>
        <rdfs:label>has basic tier</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">item that does not require any other items to be purchased</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasCurrentStatisticValue">
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
        <rdfs:domain rdf:resource="&lol;ChampionStatistic"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
        <rdfs:label>has current statistic value</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasDamageTypeString">
        <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:label>has damage type string</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasDescriptivePhrase">
        <rdfs:domain rdf:resource="&lol;Champion"/>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:label>has descriptive phrase</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasGoalStatisticValue">
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
        <rdfs:domain rdf:resource="&lol;ChampionStatistic"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
        <rdfs:label>has goal statistic value</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">statistic that one wants to maximize for a certain champion</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasGoldCost">
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:source rdf:datatype="&xsd;string">League of Legends Wiki. (2011, August). Gold. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Gold</dct:source>
        <rdfs:label>has gold cost</rdfs:label>
        <skos:definition>the cost (in the in-game currency of gold) that the current instance of the item class has to buy it</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasIncreaseRate">
        <rdfs:subPropertyOf rdf:resource="&fibo-fnd-qt-qtu;hasNumericValue"/>
        <rdfs:domain rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
        <rdfs:label>has increase rate</rdfs:label>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasItemIdentifier">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:label>has item identifier</rdfs:label>
        <skos:definition>the uniquely identifying integer value of an instance of the item class</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasLegendaryTier">
        <rdfs:subPropertyOf rdf:resource="&lol;hasTier"/>
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:label>has legendary tier</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">item that has a specified recipe that consists of at least one advanced item and a combine cost</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasLikelihood">
        <rdfs:domain rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:label>has likelihood</rdfs:label>
        <skos:definition rdf:datatype="&xsd;integer">percentage that a champion will play a certain role for any given game</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasMaximumStacks">
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:source rdf:datatype="&xsd;string">League of Legends Wiki. (2011, August). Stack. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Stack</dct:source>
        <rdfs:comment>an item can optionally have a property that stacks up to this maximum.  If the item does not stack, this value is -1.  </rdfs:comment>
        <rdfs:label>has maximum stacks</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">maximum value that instance of an effect which can multiply itself several times can have</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasMythicalTier">
        <rdfs:subPropertyOf rdf:resource="&lol;hasTier"/>
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:source rdf:datatype="&xsd;integer">League of Legends Wiki. (2011, August). Items. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Item</dct:source>
        <rdfs:label>has mythical tier</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">item that has a specified recipe that consists of at least one legendary item and a combine cost</skos:definition>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="&lol;hasTier">
        <rdfs:domain rdf:resource="&lol;Item"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <dct:source rdf:datatype="&xsd;integer">League of Legends Wiki. (2011, August). Items. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Item</dct:source>
        <rdfs:comment>All items are of tiers 1 through 4, where:
tier 1: Basic item
tier 2: Advanced item
tier 3: legendary item
tier 4: mythical item</rdfs:comment>
        <rdfs:label>has tier</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">one of four categories that an item is divided into</skos:definition>
    </owl:DatatypeProperty>
    

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:Class rdf:about="&lol;AbilityPower">
        <rdfs:subClassOf rdf:resource="&lol;MagicStatistic"/>
        <dct:alternative rdf:datatype="&xsd;string">AP</dct:alternative>
        <dct:source rdf:datatype="&xsd;string">League of Legends Wiki. (2011, August). Ability Power. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Ability_power</dct:source>
        <fibo-fnd-utl-av:explanatoryNote rdf:datatype="&xsd;string">Can be mitigated with magic resistance.</fibo-fnd-utl-av:explanatoryNote>
        <rdfs:label>ability power</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">champion statistic that increases the effectiveness of most abilities</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&lol;AbilitySpecificStatistic">
        <rdfs:subClassOf rdf:resource="&lol;LeagueOfLegendsStatistic"/>
        <rdfs:label>ability-specific statistic</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="&lol;Armor">
        <rdfs:subClassOf rdf:resource="&lol;ChampionStatistic"/>
        <rdfs:subClassOf rdf:resource="&lol;DefensiveValueStatistic"/>
        <dct:source rdf:datatype="&xsd;integer">League of Legends Wiki. (2011, August). Items. [Online]. Available: http://leagueoflegends.wikia.com/wiki/Item</dct:source>
        <rdfs:label>armor</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">statistic that reduces the damage done by incoming physical damage</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&lol;Assassin">
        <rdfs:subClassOf rdf:resource="&fibo-fnd-pty-pty;PartyInRole"/>
        <dct:source rdf:datatype="&xsd;string">T. A. Tomato. (2016, December 6). LoL Essentials | League of Legends New Player/Beginner Guide (63rd ed.) [Online]. Available:http://www.mobafire.com/league-of-legends/build/lol-essentials-league-of-legends-new-player-beginner-guide-371292</dct:source>
        <rdfs:label>assassin</rdfs:label>
        <skos:definition rdf:datatype="&xsd;string">champion type that is used for burst damage</skos:definition>
    </owl:Class>
    
    <owl:Class rdf:about="&lol;AttackDamage">
        <rdfs:subClassOf rdf:resource="&lol;ChampionStatistic"/>
        <rdfs:subClassOf rdf:resource="&lol;OffensiveValueStatistic"/>
        <dct:alternative rdf:datatype="&xsd;string">AD</dct:alternative>
        <dct:alternative rdf:datatype="&xsd;string">physical damage</dct:alternative>
